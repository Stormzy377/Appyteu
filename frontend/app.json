{
  "expo": {
    "name": "Appyteu",
    "slug": "Appyteu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "O Appyteu usa a sua localização para mostrar restaurantes próximos.",
        "NSLocationAlwaysUsageDescription": "O Appyteu usa a sua localização para mostrar restaurantes próximos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.stormzy377.Appyteu"
    },
    "package": "com.appyteu.app",
    "extra": {
      "eas": {
        "projectId": "c8b011d9-4ac7-46a4-8ab1-f61164daaf50"
      }
    }
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "plugins": [
    "expo-location",
    "expo-notifications",
    [
      "expo-image-picker",
      {
        "photosPermission": "O Appyteu precisa de acesso às suas fotos para que você possa partilhar imagens dos restaurantes."
      }
    ],
    "expo-dev-client"
  ],
  "extra": {
    "eas": {
      "projectId": "c8b011d9-4ac7-46a4-8ab1-f61164daaf50"
    }
  }
}
